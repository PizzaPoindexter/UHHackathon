<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hackathon.pizzapoindexter.uhhackathon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="DrinkingBuddy"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity" />
        <activity
            android:name=".DrinkingActivity"
            android:label="@string/title_activity_drinking"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NewDrink" />
        <activity android:name=".Beer" />
        <activity android:name=".Wine" />
        <activity android:name=".LiquorView" />
        <activity android:name=".AddOwn"></activity>
    </application>

</manifest>